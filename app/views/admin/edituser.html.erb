<h2>Edit</h2>

<%= form_for(@user, url: admin_path(@user.id), method: :patch) do |f| %>

  <div class="field">
    <%= f.label :full_name %><br />
    <%= f.text_field :full_name, autofocus: true, autocomplete: "full_name" %>
  </div>  

  <div class="actions">
    <%= f.submit "Update" %>
  </div>
<% end %>


<%= link_to "Back", :back %>
